@page
@model Torneo.App.Frontend.Pages.Municipios.IndexModel
@{
}
<h1>Listado de municipios</h1>
<table class = "table">
    <thead class="thead/dark">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var municipio in Model.municipios)
        {
            <tr>
                <th>@municipio.Id</th>
                <th>@municipio.Nombre</th>
            </tr>
            
        }
    </tbody>
</table>

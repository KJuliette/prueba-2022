@page
@model Torneo.App.Frontend.Pages.DTs.DetailsModel
@{
}
<h5 class="display-5">Detalle del Director Técnico</h5>
<div class="col-sm-12 col-md-6 col-lg-6 col-xl-6">
    <table class="table table-striped table-hover table-bordered table-secondary">
        <tr>
            <th scope="col">Id</th>
            <td>@Model.directorTecnico.Id</td>
        </tr>
        <tr>
            <th scope="col">Nombre</th>
            <td>@Model.directorTecnico.Nombre</td>
        </tr>
        <tr>
            <th scope="col">Documento</th>
            <td>@Model.directorTecnico.Documento</td>
        </tr>
        <tr>
            <th scope="col">Teléfono</th>
            <td>@Model.directorTecnico.Telefono</td>
        </tr>
    </table>
    <a class="btn btn-secondary" asp-page="./Index">Volver</a>
</div>